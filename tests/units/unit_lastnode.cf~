body common control
{
bundlesequence => {"yes"};
}


bundle agent yes
{
vars:

  "path1" string => "/one/two/last";
  "path2" string => "one:two:last";

  "last1" string => lastnode("$(path1)","/");
  "last2" string => lastnode("$(path2)",":");

  "last3" string => lastnode("$(path2)","/");

reports:

  Year2009::

    "Last = $(last1),$(last2),$(last3)";

}